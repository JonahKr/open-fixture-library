{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "ColorSource Linear 1",
  "categories": ["Color Changer"],
  "meta": {
    "authors": ["John Roper"],
    "createDate": "2021-10-25",
    "lastModifyDate": "2021-10-25"
  },
  "links": {
    "manual": [
      "https://www.etcconnect.com/WorkArea/DownloadAsset.aspx?id=10737494637"
    ],
    "productPage": [
      "https://www.etcconnect.com/Products/Lighting-Fixtures/ColorSource-Linear/Features.aspx"
    ],
    "video": [
      "https://www.youtube.com/watch?v=Fnl8hqJLAC4"
    ]
  },
  "physical": {
    "dimensions": [499, 109, 153],
    "weight": 5.1,
    "power": 115,
    "DMXconnector": "5-pin",
    "bulb": {
      "type": "LED",
      "lumens": 3039
    }
  },
  "availableChannels": {
    "Intensity": {
      "capability": {
        "type": "Intensity"
      }
    },
    "Red": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "Shutter / Strobe": {
      "capability": {
        "type": "ShutterStrobe",
        "shutterEffect": "Strobe",
        "speedStart": "slow",
        "speedEnd": "fast",
        "durationStart": "0%",
        "durationEnd": "100%"
      }
    },
    "Lime": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Lime"
      }
    }
  },
  "modes": [
    {
      "name": "5ch",
      "channels": [
        "Intensity",
        "Red",
        "Green",
        "Blue",
        "Shutter / Strobe"
      ]
    },
    {
      "name": "RGB",
      "channels": [
        "Red",
        "Green",
        "Blue"
      ]
    },
    {
      "name": "1ch",
      "channels": [
        "Intensity"
      ]
    },
    {
      "name": "Dir",
      "channels": [
        "Intensity",
        "Red",
        "Green",
        "Blue",
        "Lime",
        "Shutter / Strobe"
      ]
    }
  ]
